	catn("HOMS_CONVERSION_RATE:: "+ HOMING_PHASE_CUT_RATE);
	catn("LOW_DENSITY_GROWTH_RATE:: "+ GROWTH_AT_ZERO_DENSITY);

	
	
	//Modification detection
