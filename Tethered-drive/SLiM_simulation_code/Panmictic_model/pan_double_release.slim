	wt = all[all.countOfMutationsOfType(m1) == 2 & all.countOfMutationsOfType(m7) == 2];
	HOMSdropgen = SECOND_DROP_GEN - 11;
	catn("HOMS_DROP_GEN:: "+HOMSdropgen);
	// Add a drop of modified individuals to p1.
	if (DROP_ADDS_TO_POP){
		size_of_drop = asInteger(pop_size / 2 * DROP_PERCENTAGE_OVERRIDE_HOM / (1 - DROP_PERCENTAGE_OVERRIDE_HOM));
		sim.addSubpop("p3", size_of_drop);
		drop2 = p3.individuals;
	}
	
	if (!DROP_ADDS_TO_POP) {
		size_of_drop = asInteger(DROP_RADIUS * DROP_PERCENTAGE_OVERRIDE_HOM * pop_size);
		if (!SAME_TIME_DROP){
			drop2 = sample(all, size_of_drop);
		}
		if (SAME_TIME_DROP){
			drop2 = sample(wt, size_of_drop);
		}
	}
	
	// Now set the drop individuals to the correct genotypes:
	if (DOUBLE_SECOND_DRIVE){
		drop2.genomes.addNewDrawnMutation(c(m2, m8), c(0, 1));
	
		// For heterozygous drop, one of the chromosomes is set to wild type:
		if (HETEROZYGOUS_DROP){
			drop2.genome2.addNewDrawnMutation(c(m1, m7), c(0, 1));
		// TARE homozygotes; Homing suppression heterozygotes
		}else{
			drop2.genome2.addNewDrawnMutation(c(m2, m7), c(0, 1));
		}
	}
	if (!DOUBLE_SECOND_DRIVE){
		drop2.genomes.addNewDrawnMutation(c(m1, m8), c(0, 1));
		drop2.genome2.addNewDrawnMutation(c(m1, m7), c(0, 1));
	}

	
	// Move all the individuals from the drop into p1:
	p1.takeMigrants(drop2);
	if (DROP_ADDS_TO_POP){
			p3.removeSubpopulation();
	}
}



