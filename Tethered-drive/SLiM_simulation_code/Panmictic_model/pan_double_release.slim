}



//// DROP A GROUP OF MODIFIED INDIVIDUALS INTO THE POPULATION.
11 early(){
	all = sim.subpopulations.individuals;
	pop_size = length(all);
	
	// Add a drop of modified individuals to p1.
	if (DROP_ADDS_TO_POP){
		size_of_drop = asInteger(pop_size / 2 * DROP_PERCENTAGE_OVERRIDE_TARE / (1 - DROP_PERCENTAGE_OVERRIDE_TARE));
		sim.addSubpop("p2", size_of_drop);
		drop1 = p2.individuals;
		//p1.individualCount
	}
	
	if (!DROP_ADDS_TO_POP) {
		size_of_drop = asInteger(DROP_RADIUS * DROP_PERCENTAGE_OVERRIDE_TARE * pop_size);	
		drop1 = sample(all, size_of_drop);
	}
	
	// Now set the drop individuals to the correct genotypes:
	drop1.genomes.addNewDrawnMutation(c(m2, m7),c(0, 1));

	
	// For heterozygous drop, one of the chromosomes is set to wild type:
	if (HETEROZYGOUS_DROP){
		drop1.genome2.addNewDrawnMutation(c(m1, m7), c(0, 1));
	}

	
	// Move all the individuals from the drop into p1:
	p1.takeMigrants(drop1);
	if (DROP_ADDS_TO_POP){
		p2.removeSubpopulation();
	}

}

//// DROP A GROUP OF MODIFIED INDIVIDUALS INTO THE POPULATION.
11 early(){
	all = sim.subpopulations.individuals;
